<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.ncut.dao.AdminDao">
    <select id="selectAdminByUserId" resultType="cn.edu.ncut.domain.Admin">
        select * from admin where admin_user_id = #{userId}
    </select>

</mapper>